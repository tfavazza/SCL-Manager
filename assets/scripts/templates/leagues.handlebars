<div id="accordion" role="tablist" aria-multiselectable="true">
	{{#each this}}
	<div class="panel panel-primary col-md-12 col-sm-12 .lead">
		<span style="cursor: pointer" class="panel-primary" data-toggle="collapse" data-parent="#accordion" href="#collapse{{name}}" aria-expanded="true" aria-controls="collapse{{name}}">
			<div class="panel-heading row" role="tab" id="heading{{name}}">
				<h2 class="division-name text-center">
					{{name}}
				</h2>
			</div>				
		</span>
		<div id="collapse{{name}}" class="panel-collapse collapse" role="tabpanel" aria-labelledby="heading{{name}}">		
			{{# each players}}
			<div class="panel-heading h3 alt-color" role="tab" id="heading{{name}}">
				<span class="col-md-1 flag-icon flag-icon-{{country}}"></span>
				<div class="col-md-2">
					 {{name}}
				</div>
				<div class="col-md-2">Score {{score}}</div>
				<div class="col-md-2">Wins {{wins}}</div>
				<div class="col-md-2">Losses {{losses}}</div>
				<div class="col-md-2">Draws {{draws}}</div>
				<button class="btn btn-small btn-info col-md-1" data-target="#player-modal" id="{{name}}" data-toggle="modal">Schedule</button>
				<br>
			</div>
			{{/each}}
		</div>
	</div>
	{{/each}}
</div>